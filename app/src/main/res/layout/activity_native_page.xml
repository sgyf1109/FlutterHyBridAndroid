<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".NativePageActivity">
    <TextView
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/tv"
        android:gravity="center"
        android:text="我是原生页面"
        android:layout_width="match_parent"
        android:layout_height="45dp"/>
    <Button
        android:text="返回上一页"
        app:layout_constraintTop_toBottomOf="@id/tv"
        android:id="@+id/btnBack"
        android:layout_width="match_parent"
        android:layout_height="45dp"/>
    <Button
        android:text="向flutter发送消息"
        app:layout_constraintTop_toBottomOf="@id/btnBack"
        android:id="@+id/basicMessageChannel"
        android:layout_width="match_parent"
        android:layout_height="45dp"/>
    <Button
        android:text="向flutter发送消息并接受回调"
        app:layout_constraintTop_toBottomOf="@id/basicMessageChannel"
        android:id="@+id/basicMessageChannel2"
        android:layout_width="match_parent"
        android:layout_height="45dp"/>
</androidx.constraintlayout.widget.ConstraintLayout>